<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <home>

     <!-- Left screen [1] -->
     <appwidget
        launcher:packageName="flipboard.app"
        launcher:className="flipboard.widget.FlipboardWidgetMedium"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="4"
        launcher:spanY="3" />

     <!-- Kids Mode -->
     <appwidget
        launcher:packageName="com.sec.kidsplat.installer"
        launcher:className="com.sec.kidsplat.installer.KidsModeInstallerWidget"
        launcher:screen="0"
        launcher:x="0"
        launcher:y="3"
        launcher:spanX="1"
        launcher:spanY="1" />

     <favorite
        launcher:packageName="tv.peel.smartremote"
        launcher:className="com.peel.main.Main"
        launcher:screen="0"
        launcher:x="1"
        launcher:y="3" /> 

     <favorite
        launcher:packageName="com.vlingo.midas" 
        launcher:className="com.vlingo.midas.gui.ConversationActivity" 
        launcher:screen="0"
        launcher:x="2"
        launcher:y="3" /> 

     <favorite
        launcher:packageName="com.sec.android.mimage.sstudio" 
        launcher:className="com.sec.android.mimage.sstudio.StudioActivity"
        launcher:screen="0"
        launcher:x="3"
        launcher:y="3" />
     <!-- End of left screen [1] -->


     <!-- Midle screen [2] -->   
     <sactivitywidget 
	launcher:packageName="com.sec.android.widgetapp.ap.hero.accuweather" 
	launcher:className="com.sec.android.widgetapp.ap.hero.accuweather.SurfaceWidgetWeather"
	launcher:screen="1"
	launcher:x="0"
	launcher:y="0"
	launcher:spanX="4"
	launcher:spanY="2" />

     <appwidget
        launcher:packageName="com.google.android.googlequicksearchbox"
        launcher:className="com.google.android.googlequicksearchbox.SearchWidgetProvider"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="2"
        launcher:spanX="4"
        launcher:spanY="1" />

     <favorite
        launcher:packageName="com.sec.android.app.camera"
        launcher:className="com.sec.android.app.camera.Camera"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="3" /> 
      <!-- Galaxy Apps -->
     <favorite
        launcher:packageName="com.sec.android.app.samsungapps"
        launcher:className="com.sec.android.app.samsungapps.SamsungAppsMainActivity"
        launcher:screen="1"
        launcher:x="1"
        launcher:y="3" /> 

     <favorite
        launcher:packageName="com.android.vending"
        launcher:className="com.android.vending.AssetBrowserActivity"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="3" /> 

      <!-- Google -->
      <folder
        launcher:screen="1"
        launcher:x="3"
        launcher:y="3" 
        launcher:title="Google" >

	    <!-- Chrome -->
            <favorite
                launcher:screen="0"
                launcher:packageName="com.android.chrome"
                launcher:className="com.google.android.apps.chrome.Main" />
            <!-- Gmail -->
            <favorite
                launcher:screen="1"
                launcher:packageName="com.google.android.gm"
                launcher:className="com.google.android.gm.ConversationListActivityGmail" />
            <!-- Google+ -->
            <favorite
                launcher:screen="2"
                launcher:packageName="com.google.android.apps.plus"
                launcher:className="com.google.android.apps.plus.phone.HomeActivity" />
            <!-- Maps -->
            <favorite
                launcher:screen="3"
                launcher:packageName="com.google.android.apps.maps"
                launcher:className="com.google.android.maps.MapsActivity" />
            <!-- Play Movies -->
            <favorite
                launcher:screen="4"
                launcher:packageName="com.google.android.videos"
                launcher:className="com.google.android.youtube.videos.EntryPoint" />
            <!-- Play Books -->
            <favorite
                launcher:screen="5"
                launcher:packageName="com.google.android.apps.books"
                launcher:className="com.google.android.apps.books.app.BooksActivity" />
            <!-- Play Magazines -->
            <favorite
                launcher:screen="6"
                launcher:packageName="com.google.android.apps.magazines"
                launcher:className="com.google.apps.dots.android.app.activity.CurrentsStartActivity" />
            <!-- Play Games -->
            <favorite
                launcher:screen="7"
                launcher:packageName="com.google.android.play.games"
                launcher:className="com.google.android.gms.games.ui.destination.main.MainActivity" />
            <!-- Drive -->
            <favorite
                launcher:screen="8"
                launcher:packageName="com.google.android.apps.docs"
                launcher:className="com.google.android.apps.docs.app.NewMainProxyActivity" />
            <!-- YouTube -->
            <favorite
                launcher:screen="9"
                launcher:packageName="com.google.android.youtube"
                launcher:className="com.google.android.youtube.app.honeycomb.Shell$HomeActivity" />
            <!-- Photos -->
            <favorite
                launcher:screen="10"
                launcher:packageName="com.google.android.apps.plus"
                launcher:className="com.google.android.apps.plus.phone.ConversationListActivity" />
            <!-- Hangouts -->
            <favorite
                launcher:screen="11"
                launcher:packageName="com.google.android.talk"
                launcher:className="com.google.android.talk.SigningInActivity" />
     </folder>
     <!-- End of midle screen [2] -->  



     <!-- Right screen [3] -->  
     <appwidget
        launcher:packageName="br.org.sidi.aplicacoesbrasil.widget"
        launcher:className="br.org.sidi.aplicacoesbrasil.widget.WidgetListener"
        launcher:screen="2"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="4"
        launcher:spanY="2" />

     <favorite
        launcher:packageName="com.dropbox.android"
        launcher:className="com.dropbox.android.activity.DropboxBrowser"
        launcher:screen="2"
        launcher:x="0"
        launcher:y="3" /> 

     <favorite
        launcher:packageName="com.evernote" 
        launcher:className="com.evernote.ui.HomeActivity"
        launcher:screen="2"
        launcher:x="1"
        launcher:y="3" /> 

     <!--Play Music-->
     <favorite 
	launcher:packageName="com.google.android.music" 
	launcher:className="com.android.music.activitymanagement.TopLevelActivity" 
	launcher:screen="2"
	launcher:x="2" 
	launcher:y="3"  />

     <favorite
        launcher:packageName="com.gameloft.android.LATAM.GloftAA03"
        launcher:className="com.gameloft.android.LATAM.GloftAA03.StartUpdate"
        launcher:screen="2"
        launcher:x="3"
        launcher:y="3" /> 



     <!-- End of right screen [3] --> 

    </home>    
	
    <!-- Hotseat (We use the screen as the position of the item in the hotseat) -->
    <hotseat>
        <!-- Phone -->
        <favorite
            launcher:packageName="com.android.contacts" 
            launcher:className="com.android.dialer.DialtactsActivity" 
            launcher:screen="0" />
        <!-- Contacts -->
        <favorite
            launcher:packageName="com.android.contacts" 
            launcher:className="com.android.contacts.activities.PeopleActivity" 
            launcher:screen="1" />
        <!-- Messaging -->
        <favorite
            launcher:packageName="com.android.mms" 
            launcher:className="com.android.mms.ui.ConversationComposer" 
            launcher:screen="2" />
        <!-- Internet -->
        <favorite
            launcher:packageName="com.sec.android.app.sbrowser" 
            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity" 
            launcher:screen="3" />
    </hotseat>  
</favorites>
